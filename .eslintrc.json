{
  "env":{
     "es2017": true,
     "node": true
  },
  "extends":[
     "airbnb-base",
     "plugin:@typescript-eslint/recommended",
     "plugin:prettier/recommended"
  ],
  "parser":"@typescript-eslint/parser",
  "parserOptions":{
     "ecmaVersion":2018,
     "sourceType":"module"
  },
  "plugins":[
     "@typescript-eslint",
     "prettier"
  ],
  "rules":{
    "prettier/prettier": ["error", {
      "endOfLine":"auto"
     }]
  }
}